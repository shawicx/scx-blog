---
export interface Props {
  class?: string;
  fill?: string;
  'aria-hidden'?: boolean | "false" | "true" | null | undefined;
}

const { class: className, fill = 'currentColor', 'aria-hidden': ariaHidden, ...props } = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  viewBox="0 0 24 24"
  fill={fill}
